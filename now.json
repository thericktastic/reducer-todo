{
    "version": 2,
    "name": "project",
    "builds": [
        {
            "src": "project/package.json",
            "use": "@now/static-build",
            "config": {
                "distDir": "build"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "project/$1"
        }
    ]
}